{
    "defaultSeverity": "error",
    "extends": "tslint-config-airbnb",
    "jsRules": {},
    "rules": {
        "trailing-comma": [
            true,
            {
                "multiline": {
                "arrays": "never",
                "objects": "never",
                "functions": "never",
                "typeLiterals": "ignore"
                },
                "esSpecCompliant": true
            }
            ]
    },
    "rulesDirectory": []
}